      "source": [
        "def calculate_tax(price, tax_rate):\n",
        "    total = price + (price * (tax_rate / 100))  # 税率は％で計算\n",
        "    return total  # 計算結果を返す\n",
        "\n",
        "\n",
        "price = 110       # 商品金額\n",
        "tax_rate = 10     # 消費税率（10%）\n",
        "\n",
        "result = calculate_tax(price, tax_rate)\n",
        "print(\"税込金額:\", result)"
      ]
    }
